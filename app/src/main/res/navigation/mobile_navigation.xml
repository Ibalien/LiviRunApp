<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="monapp.dreams.com.myapplication.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_magasin"
        android:name="monapp.dreams.com.myapplication.MagasinFragment"
        android:label="@string/title_magasin"
        tools:layout="@layout/magasin_item_list" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="monapp.dreams.com.myapplication.LivraisonFragment"
        android:label="@string/title_livraison"
        tools:layout="@layout/livraison_item_list" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="monapp.dreams.com.myapplication.ui.notifications.NotificationsFragment"
        android:label="@string/title_parametre"
        tools:layout="@layout/fragment_notifications" />


</navigation>